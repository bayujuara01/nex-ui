    "@tanstack/react-virtual": "3.11.2"
  },
  "peerDependencies": {
    "@mantine/core": "~7.15.x",
    "@mantine/dates": "~7.15.x",
    "@mantine/hooks": "~7.15.x",
    "@tabler/icons-react": ">=2.23.0",
    "clsx": ">=2",
    "dayjs": ">=1.11",
    "react": ">=18.0",
    "react-dom": ">=18.0"
  }
}
