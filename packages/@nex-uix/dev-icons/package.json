{
  "name": "@nex-uix/dev-icons",
  "version": "1.1.0",
  "description": "Dev icons used on *.nex-ui.dev websites",
  "homepage": "https://nex-ui.dev/",
  "license": "MIT",
  "author": "Bayu Seno Ariefyanto <ariefyantobayu.dev@gmail.com>",
  "keywords": [
    "react",
    "ui"
  ],
  "sideEffects": [
    "*.css"
  ],
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    }
  },
  "repository": {
    "url": "https://github.com/bayujuara01/nex-ui.git",
    "type": "git",
    "directory": "packages/@nex-uix/dev-icons"
  },
  "peerDependencies": {
    "@nex-ui/core": ">=7.0.0",
    "@nex-ui/hooks": ">=7.0.0",
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "devDependencies": {
    "@nex-ui-tests/core": "workspace:*",
    "@nex-ui/core": "workspace:*",
    "@nex-ui/hooks": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
